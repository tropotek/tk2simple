{
    "name": "ttek/tksimple",
    "type": "project",
    "description": "A simple Tk Ver 2 framework project",
    "keywords":  ["tropotek"],
    "time": "2019-04-09",
    "license": "MIT",
    "version": "master",
    "minimum-stability": "dev",
    "authors":  [
         {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload":  {
        "psr-0":  {
            "": "src/"
        }
    },
    "scripts":  {
        "post-install-cmd":  [
            "Tk\\Composer\\InitProjectEvent::postInstall"
        ],
        "post-update-cmd":  [
            "Tk\\Composer\\InitProjectEvent::postUpdate"
        ]
