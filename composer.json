{
    "name": "ttek/tksimple",
    "type": "project",
    "description": "A simple Tk Ver 2 framework project",
    "keywords":  [
        "tropotek"
    ],
    "time": "2019-05-11",
    "license": "MIT",
    "version": "3.0.0",
    "minimum-stability": "stable",
    "authors":  [
         {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload":  {
        "psr-0":  {
            "": "src/"
        }
    },
    "scripts":  {
        "post-install-cmd":  [
            "Tk\\Composer\\InitProjectEvent::postInstall"
        ],
        "post-update-cmd":  [
            "Tk\\Composer\\InitProjectEvent::postUpdate"
        ]
    },
    "extra":  {
        "branch-alias":  {
            "dev-master": "3.0.x-dev"
        }
    },
    "require":  {
        "php": "^7.1.3",
        "ttek/tk-framework": "dev-3beta as 3.0.x-dev",
        "ttek/tk-installers": "dev-3beta as 3.0.x-dev",
        "ttek/tk-database": "dev-3beta as 3.0.x-dev",
        "ttek/tk-domtemplate": "dev-3beta as 3.0.x-dev",
        "ttek/tk-form": "dev-3beta as 3.0.x-dev",
        "ttek/tk-table": "dev-3beta as 3.0.x-dev",
        "ttek/tk-mail": "dev-3beta as 3.0.x-dev"
    },
    "require-dev":  {
    }
}