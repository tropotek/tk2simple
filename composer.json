            "dev-master": "3.0.x-dev"
        }
    },
    "require":  {
        "php": "^7.1.3",

        "ttek/tk-framework": "~3.0",
        "ttek/tk-installers": "~3.0",
        "ttek/tk-database": "~3.0",
        "ttek/tk-domtemplate": "~3.0",
        "ttek/tk-form": "~3.0",
        "ttek/tk-table": "~3.0",
        "ttek/tk-mail": "~3.0"
    },
    "require-dev":  {
    }
}
